{
  "buildCommand": "NODE_OPTIONS='--max-old-space-size=4096' pnpm install && pnpm run vercel-build",
  "framework": "nextjs",
  "installCommand": "pnpm install",
  "outputDirectory": ".next",
  "regions": ["icn1"],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9",
      "PIP_TARGET": ".",
      "PIP_DISABLE_PIP_VERSION_CHECK": "1",
      "PYTHONPATH": ".",
      "PYTHONUNBUFFERED": "1"
    }
  }
}
