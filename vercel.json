{
  "buildCommand": "NODE_OPTIONS='--max-old-space-size=4096' pnpm install && pnpm run vercel-build",
  "framework": "nextjs",
  "installCommand": "pnpm install",
  "outputDirectory": ".next",
  "regions": ["icn1"],
  "functions": {
    "api/python/*.py": {
      "runtime": "@vercel/python@3.1.0",
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
