{
  "buildCommand": "NODE_OPTIONS='--max-old-space-size=4096' pnpm install && pnpm run vercel-build",
  "framework": "nextjs",
  "installCommand": "pnpm install",
  "outputDirectory": ".next",
  "regions": ["icn1"],
  "functions": {
    "api/python/*.py": {
      "runtime": "@vercel/python@3.12",
      "pythonVersion": "3.12",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "env": {
    "PYTHON_VERSION": "3.12",
    "PYTHON_RUNTIME": "3.12"
  }
}
